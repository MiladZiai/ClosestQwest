<div>
	{{#if errors}}
        <ul>
            {{#each errors}}
                <li>{{this}}</li>
            {{/each}}
        </ul>
	{{else}}
		{{#if accounts}}
            <h1>All Accounts: </h1>
            <ul>
                {{#each accounts}}
                    <li>{{username}}</li>
                {{/each}}
            </ul>
		{{else}}
		    <p>No accounts exists.</p>
		{{/if}}
	{{/if}}
</div>

